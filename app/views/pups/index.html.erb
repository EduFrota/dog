<p id="notice"><%= notice %></p>

<h1>Pups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Breed</th>
      <th>Age</th>
      <th>Genre</th>
      <th>Vaccine</th>
      <th>Pedigree</th>
      <th>Mothername</th>
      <th>Namefather</th>
      <th>Rg</th>
      <th>Temperament</th>
      <th>Length</th>
      <th>Alturacernelha</th>
      <th>Tipopelo</th>
      <th>Corpelo</th>
      <th>Weigth</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pups.each do |pup| %>
      <tr>
        <td><%= pup.name %></td>
        <td><%= pup.breed %></td>
        <td><%= pup.age %></td>
        <td><%= pup.genre %></td>
        <td><%= pup.vaccine %></td>
        <td><%= pup.pedigree %></td>
        <td><%= pup.mothername %></td>
        <td><%= pup.namefather %></td>
        <td><%= pup.rg %></td>
        <td><%= pup.temperament %></td>
        <td><%= pup.length %></td>
        <td><%= pup.alturacernelha %></td>
        <td><%= pup.tipopelo %></td>
        <td><%= pup.corpelo %></td>
        <td><%= pup.weigth %></td>
        <td><%= link_to 'Show', pup %></td>
        <td><%= link_to 'Edit', edit_pup_path(pup) %></td>
        <td><%= link_to 'Destroy', pup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pup', new_pup_path %>
